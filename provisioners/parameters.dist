# Use CentOS 7 (64-bit) box
$box_name = "ubuntu/trusty64"
$box_url = "https://vagrantcloud.com/ubuntu/boxes/trusty64/versions/14.04/providers/virtualbox.box"
$box_distrib = "ubuntu"
 
# VM settings
$vm_hostname = "sfynx.jenkins.vagrant"
$vm_name = "JENKINS"
$vm_group = "/vagrant"
 
# VM configuration and capacities
$vm_gui = false # Enable or not GUI
$vm_www_point = "/var/www"
$vm_memory = "2048"
$vm_cpus = "2"
$vm_cpu_cap = "100"
$vm_ip = "192.168.33.30"